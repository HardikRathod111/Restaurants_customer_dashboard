{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "routes/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "controllers/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "models/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "/uploads/(.*)",
        "dest": "/uploads/$1"
      }
      
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      }
    ]
  }
  
